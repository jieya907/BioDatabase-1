<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


    <servlet>
        <servlet-name>postManual</servlet-name>
        <servlet-class>PostManualServlet</servlet-class>

    </servlet>
    <servlet>
        <servlet-name>searchManual</servlet-name>
        <servlet-class>SearchManualServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>postManual</servlet-name>
        <url-pattern>/post_manual</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchManual</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>
